{
	"name": "awesome-vjass-support",
	"displayName": "awesome vjass support",
	"description": "vjass support for warcraft's worldeditor.",
	"repository": {
		"type": "git",
		"url": "https://github.com/FakedWeiss/awesome-vjass-support.git"
	},
	"version": "0.0.92",
	"publisher": "FakedWeiss",
	"engines": {
		"vscode": "^1.20.0"
	},
	"categories": [
		"Languages"
	],
	"keywords": [
		"JASS",
		"vJASS",
		"Syntax",
		"Warcraft"
	],
	"activationEvents": [
		"onLanguage:vjass"
	],
	"main": "./extension",
	"contributes": {
		"languages": [
			{
				"id": "vjass",
				"aliases": [
					"vJASS",
					"vjass"
				],
				"extensions": [
					".jass",
					".j",
					".ai"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "vjass",
				"scopeName": "source.vjass",
				"path": "./syntaxes/vjass.json"
			}
		],
		"snippets": [
			{
				"language": "vjass",
				"path": "./snippets/vj_snippets.json"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.6",
		"eslint": "^4.6.1",
		"@types/node": "^7.0.43",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"jsdom": "^11.3.0",
		"tmp": "0.0.33",
		"vscode-ripgrep": "^0.7.1-patch.1.5"
	},
	"__metadata": {
		"id": "03c89e4c-6bb3-43af-841b-8268d875c114",
		"publisherDisplayName": "FakedWeiss",
		"publisherId": "12023217-c3cb-48d6-9a4c-dc550af721f4"
	}
}